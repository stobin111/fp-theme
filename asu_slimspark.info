scripts[] = 'js/affix.js'
scripts[] = 'js/alert.js'
scripts[] = 'js/bootstrap.js'
scripts[] = 'js/button.js'
scripts[] = 'js/carousel.js'
scripts[] = 'js/collapse.js'
scripts[] = 'js/dropdown.js'
scripts[] = 'js/lightningtouch.js'
scripts[] = 'js/modal.js'
